			"name": "Login  (admin)",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"var xsrfCookie = postman.getResponseCookie(\"csrftoken\"); \r",
							"postman.setEnvironmentVariable('csrftoken', xsrfCookie.value);\r",
							"\r",
							"var sID = postman.getResponseCookie(\"sessionid\"); \r",
							"postman.setEnvironmentVariable('sessionid', sID.value);"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "formdata",
					"formdata": [
						{
							"key": "username",
							"value": "admin",
							"type": "text"
						},
						{
							"key": "password",
							"value": "admin",
							"type": "text"
						},
						{
							"key": "csrfmiddlewaretoken",
							"value": "{{csrftoken}}",
							"type": "text"
						}
					]
				},
				"url": {
					"raw": "http://127.0.0.1:8000/api-auth/login/?next=/api/v1/pages/list",
